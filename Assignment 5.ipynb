    "class power:\n",
    "    def __init__(self,x,n):\n",
    "        self.x= int(input(\"enter num x: \"))\n",
    "        self.n= int(input(\"enter num n: \"))\n",
    "        \n",
    "    def nthpow(self):\n",
    "        print(self.n,\"th power of\", self.x ,\":\", self.x**self.n)\n",
    "ob=power(\"x\",\"n\")\n",
    "ob.nthpow()\n"
